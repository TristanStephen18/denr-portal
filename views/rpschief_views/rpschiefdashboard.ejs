<!DOCTYPE html>
<html lang="en">
  <%- include('../partials/head', {title: 'DENR PORTAL RPS',
  csslink:'../css/rpschief/rpschief.css'}) %>
  <body>
    <div class="main-content">
      <%- include('../partials/topbar') %>

      <h2>Permits from RPU for Recommending Approval</h2>
      <br>
      <div class="table-container">
        <%- include('../partials/tables/table_partials_d') %> <%-
        include('../partials/tables/evaluatedtable') %>
      </div>
    </div>
    <%- include('../partials/modals/rpschief_modal') %>
    <script
      src="../scripts/rpschief_scripts/dashboard.js"
      type="module"
    ></script>

    <script>
      const modal = document.getElementById("evaluatedmodal");
      document.getElementById("closeModal").addEventListener("click", () => {
        modal.style.display = "none";
      });
      window.addEventListener("click", (e) => {
        if (e.target === modal) {
          modal.style.display = "none";
        }
      });
    </script>
  </body>
</html>
